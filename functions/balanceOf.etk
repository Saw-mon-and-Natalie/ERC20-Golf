balanceOf:
    jumpdest

    %push(storeAndReturn)
    
    # owner
    push1 4
    calldataload

    %push(balanceOfSlot)
    jump
    

# stack: address, return_pc
balanceOfSlot:
    jumpdest

    returndatasize
    mstore

    # storage slot 0
    returndatasize
    push1 0x20
    mstore

    push1 0x40
    returndatasize
    keccak256
    
    swap1
    jump

storeAndReturn:
    jumpdest

    sload
   
    returndatasize
    mstore

    push1 0x20
    returndatasize
    return