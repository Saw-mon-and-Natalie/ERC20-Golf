# stack: 4, 1, 0x20 

balanceOf:
    jumpdest
    
    # owner
    dup1                                # 4, 4, 1, 0x20
    calldataload                        # owner, 4, 1, 0x20

    # owner address is also the storage slot id in our case

    sload                               # b[..], 4, 1, 0x20
   
    %push(returnValue)
    jump                                # b[..], 4, 1, 0x20